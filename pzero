/* Java program to push zeroes to end of the array */
class pzero
{
	void method1(int a[],int r)
	{
		int res[]=new int[r];
		int k=r-1,m=0;
		for(int j=0;j<r;j++)
		{
			if(a[j]!=0)
			{
				res[m]=a[j];
				m++;
			}
			else
			{
				res[k]=0;
				k--;
			}
		}
		for(int i=0;i<r;i++)
		System.out.println(res[i]);
	}
}
public class pushzero {
	public static void main(String[] args) {
		pzero pz = new pzero();
		int[] a = new int[] {0,2,6,4,7,0,5,0};
		int r=8;
		pz.method1(a, r);
	}
}
